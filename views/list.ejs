<%- include("header") -%>

<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<% tickets.forEach(function(ticket){ %>
<p><%= ticket.title %> &nbsp <%= ticket.content %></p>

<% }); %>

<form class="" action="/tickets" method="post">
  <input
    type="text"
    name="title"
    placeholder="Ticket Title"
    autocomplete="off"
  />
  <input
    type="text"
    name="content"
    placeholder="Ticket Content"
    autocomplete="off"
  />

  <button type="submit" name="ticket" value="">+</button>
</form>

<%- include("footer") -%>
